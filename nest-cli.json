      "compilerOptions": {
        "tsConfigPath": "apps/auditing-service/tsconfig.app.json"
      }
    },
    "authentication-service": {
      "type": "application",
      "root": "apps/authentication-service",
      "entryFile": "main",
      "sourceRoot": "apps/authentication-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/authentication-service/tsconfig.app.json"
      }
    },
    "booking-service": {
      "type": "application",
      "root": "apps/booking-service",
      "entryFile": "main",
