      "compilerOptions": {
        "tsConfigPath": "apps/external-api-integration-service/tsconfig.app.json"
      }
    },
    "hotels-search-service": {
      "type": "application",
      "root": "apps/hotels-search-service",
      "entryFile": "main",
      "sourceRoot": "apps/hotels-search-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/hotels-search-service/tsconfig.app.json"
